{
  "name": "pingtopass-nuxt",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "type": "module",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx scripts/seed-database.ts",
    "db:seed:questions": "tsx scripts/seed-static-questions.ts",
    "db:reset": "tsx scripts/reset-database.ts",
    "typecheck": "nuxi typecheck",
    "lint": "eslint .",
    "test": "vitest",
    "test:e2e": "playwright test",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest run",
    "deploy": "nuxt build && npx wrangler pages deploy .output/public --project-name=pingtopass-nuxt",
    "deploy:preview": "nuxt build && npx wrangler pages deploy .output/public --project-name=pingtopass-nuxt --branch=preview",
    "hub:link": "npx nuxthub link",
    "hub:deploy": "npx nuxthub deploy",
    "cf:login": "npx wrangler login",
    "cf:tail": "npx wrangler pages tail pingtopass-nuxt"
  },
  "devDependencies": {
